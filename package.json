{
  "name": "serviceberry-json",
  "version": "1.1.0",
  "description": "JSON serialization plugin for serviceberry",
  "main": "src/plugin.js",
  "scripts": {
    "build": "npm run lint && npm run test-coverage",
    "lint": "eslint src/**",
    "lint:spec": "eslint spec/**",
    "test": "jasmine",
    "test-coverage": "nyc --include=src/**/*.js --reporter=lcov --reporter=text npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bob-gray/serviceberry-json.git"
  },
  "keywords": [
    "json",
    "serialization",
    "serviceberry",
    "api",
    "rest",
    "http"
  ],
  "author": "Bob Gray <gray.bob98@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bob-gray/serviceberry-json/issues"
  },
  "homepage": "https://github.com/bob-gray/serviceberry-json#readme",
  "peerDependencies": {
    "serviceberry": ">=0.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.1.0",
    "jasmine": "^3.5.0",
    "jasmine-spec-reporter": "^5.0.2",
    "node-mocks-http": "^1.8.1",
    "nyc": "^15.0.1",
    "serviceberry": "^0.9.0"
  }
}
